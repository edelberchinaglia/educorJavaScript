//operdores de incremento / decremento
let n1=10
let n2=5
document.write(n1++ +"<br>")
document.write(n1++ +"<br>")
document.write(n1++ +"<br>")
document.write(--n1 +"<br>")

//operadores relacionais
document.write((n1>n2)+"<br>")
document.write((n1<n2)+"<br>")
document.write((n1==n2)+"<br>")
document.write((n1!=n2)+"<br>")
document.write((n1>=n2)+"<br>")
document.write((n1<=n2)+"<br>")

//operadores lógicos
//&& = e
//|| = ou
//!  = não



